#Parametros utilizados: 

#Numero de Tags Iniciais: 100
#Numero de Passo de Tags: 100
#Numero de Maximo de Tags: 1000
#Numero de Iteracoes: 10
#Tamanho Inicial do Quadro: 30

set xlabel "Etiquetas"
set terminal wxt size 800,800
set grid

#Total de Slots
set terminal wxt 1
set ylabel "Slots"
set title "Total de Slots"
set yrange [0:1000]plot "EomLee.txt" using 1:2 title "EomLee" smooth unique with linespoints,"Qsim.txt" using 1:2 title "Qsim" smooth unique with linespoints,"Qsim2.txt" using 1:2 title "Qsim2" smooth unique with linespoints

#Total de Slots Vazios
set terminal wxt 2
set ylabel "Slots"
set title "Total de Slots Vazios"
set yrange [0:1000]plot "EomLee.txt" using 1:3 title "EomLee" smooth unique with linespoints,"Qsim.txt" using 1:3 title "Qsim" smooth unique with linespoints,"Qsim2.txt" using 1:3 title "Qsim2" smooth unique with linespoints

#Total de Colisoes
set terminal wxt 3
set ylabel "Colisoes"
set title "Total de Colisoes"
set yrange [*:*]plot "EomLee.txt" using 1:4 title "EomLee" smooth unique with linespoints,"Qsim.txt" using 1:4 title "Qsim" smooth unique with linespoints,"Qsim2.txt" using 1:4 title "Qsim2" smooth unique with linespoints