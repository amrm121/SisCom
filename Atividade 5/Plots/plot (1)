set xlabel "Etiquetas"
set terminal wxt size 800,800
set key left top
set grid
unset log

#Total de Slots
set terminal wxt 1
set ylabel "Numero de Slots"
set title "Total de Slots"
set yrange [0:3500]
set ytics 500
plot "LowerBound.txt" using 1:2 title "Lower Bound" smooth unique with linespoints pt 2,"Schoute.txt" using 1:2 title "Schoute" smooth unique with linespoints pt 4, "EomLee.txt" using 1:2 title "Eom-Lee" smooth unique with linespoints pt 6, "AlgoritmoQ.txt" using 1:2 title "Algoritmo Q" smooth unique with linespoints pt 8, "Chen.txt" using 1:2 title "Chen" smooth unique with linespoints pt 12 lt rgb "red", "Vahedi.txt" using 1:2 title "Vahedi" smooth unique with linespoints pt 14

#Total de Slots Vazios
set terminal wxt 2
set ylabel "Numero de Slots Vazios"
set title "Total de Slots Vazios"
set yrange [0:1100]
set ytics 100
plot "LowerBound.txt" using 1:3 title "Lower Bound" smooth unique with linespoints pt 2,"Schoute.txt" using 1:3 title "Schoute" smooth unique with linespoints pt 4, "EomLee.txt" using 1:3 title "Eom-Lee" smooth unique with linespoints pt 6, "AlgoritmoQ.txt" using 1:3 title "Algoritmo Q" smooth unique with linespoints pt 8, "Chen.txt" using 1:3 title "Chen" smooth unique with linespoints pt 12 lt rgb "red", "Vahedi.txt" using 1:3 title "Vahedi" smooth unique with linespoints pt 14


#Total de Colisoes
set terminal wxt 3
set ylabel "Numero de Colisoes"
set title "Total de Colisoes"
set yrange [0:1800]
set ytics 200
plot "LowerBound.txt" using 1:4 title "Lower Bound" smooth unique with linespoints pt 2,"Schoute.txt" using 1:4 title "Schoute" smooth unique with linespoints pt 4, "EomLee.txt" using 1:4 title "Eom-Lee" smooth unique with linespoints pt 6, "AlgoritmoQ.txt" using 1:4 title "Algoritmo Q" smooth unique with linespoints pt 8, "Chen.txt" using 1:4 title "Chen" smooth unique with linespoints pt 12 lt rgb "red", "Vahedi.txt" using 1:4 title "Vahedi" smooth unique with linespoints pt 14 
